<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hzlinxuanxuan.materialdesignsample">

    <uses-permission android:name="android.permission.WRITE_SETTINGS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".QQSlideMenuActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".CommonSlideMenuActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".DrawerSlideMenuActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".ContentSlideMenuActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".CustomContentSlideMenuActivity"
            android:label="@string/app_name" />

    </application>

</manifest>
